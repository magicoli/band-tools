{
  "repositories": {
    "wpackagist": {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  },
  "update-with-our-license-key-then-add-to-repositories": {
    "metabox": {
      "type": "composer",
      "url": "https://packages.metabox.io/YOUR_META_BOX_LICENSE_KEY"
    }
  },
  "require": {
    "wpackagist-plugin/meta-box": "dev-trunk",
    "meta-box/meta-box-group": "dev-master",
    "meta-box/mb-settings-page": "dev-master",
    "afragen/wp-dependency-installer": "^3.1"
  },
  "extra": {
    "installer-paths": {
      "vendor/meta-box/meta-box": "wpackagist-plugin/meta-box",
      "vendor/meta-box/meta-box-group": "meta-box/meta-box-group",
      "vendor/meta-box/mb-settings-page": "meta-box/mb-settings-page"
    }
  },
  "autoload": {
    "files": [
      "vendor/meta-box/meta-box/meta-box.php",
      "vendor/meta-box/meta-box-group/meta-box-group.php",
      "vendor/meta-box/mb-settings-page/mb-settings-page.php",
      "vendor/afragen/wp-dependency-installer/wp-dependency-installer.php"
    ]
  }
}
